{
    "configurations": [
        {
            "name": "MSVC",
            "includePath": [
                "${workspaceFolder}/**",
                "${env:VISUAL_STUDIO_HOME}/include"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "windowsSdkVersion": "10.0.18362.0",
            "compilerPath": "${env:VISUAL_STUDIO_BIN}/cl.exe",
            "cStandard": "c11",
            "intelliSenseMode": "msvc-x64"
        },
        {
            "name": "GNU G++",
            "includePath": [
                "${workspaceFolder}/**",
                "${env:MINGW_HOME}/include"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "windowsSdkVersion": "10.0.18362.0",
            "compilerPath": "${env:MINGW_HOME}/bin/g++.exe",
            "cStandard": "c11",
            "intelliSenseMode": "gcc-x64",
            "compilerArgs": [],
            "cppStandard": "c++17",
            "browse": {
                "path": [
                    "${workspaceFolder}/**"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ],
    "version": 4
}